      "def get_jk(mol, dm, hermi=1, vhfopt=None, with_j=True, with_k=True, omega=None):\n",
      "    '''Compute J, K matrices for all input density matrices\n",
      "\n",
      "    Args:\n",
      "        mol : an instance of :class:`Mole`\n",
      "\n",
      "        dm : ndarray or list of ndarrays\n",
      "            A density matrix or a list of density matrices\n",
      "\n",
      "    Kwargs:\n",
      "        hermi : int\n",
      "            Whether J, K matrix is hermitian\n",
      "\n",
      "            | 0 : not hermitian and not symmetric\n",
      "            | 1 : hermitian or symmetric\n",
      "            | 2 : anti-hermitian\n",
      "\n",
      "        vhfopt :\n",
      "            A class which holds precomputed quantities to optimize the\n",
      "            computation of J, K matrices\n",
      "\n",
      "        with_j : boolean\n",
      "            Whether to compute J matrices\n",
      "\n",
      "        with_k : boolean\n",
      "            Whether to compute K matrices\n",
      "\n",
      "        omega : float\n",
      "            Parameter of range-seperated Coulomb operator: erf( omega * r12 ) / r12.\n",
      "            If specified, integration are evaluated based on the long-range\n",
      "            part of the range-seperated Coulomb operator.\n",
      "\n",
      "    Returns:\n",
      "        Depending on the given dm, the function returns one J and one K matrix,\n",
      "        or a list of J matrices and a list of K matrices, corresponding to the\n",
      "        input density matrices.\n",
      "\n",
      "    Examples:\n",
      "\n",
      "    >>> from pyscf import gto, scf\n",
      "    >>> from pyscf.scf import _vhf\n",
      "    >>> mol = gto.M(atom='H 0 0 0; H 0 0 1.1')\n",
      "    >>> dms = numpy.random.random((3,mol.nao_nr(),mol.nao_nr()))\n",
      "    >>> j, k = scf.hf.get_jk(mol, dms, hermi=0)\n",
      "    >>> print(j.shape)\n",
      "    (3, 2, 2)\n",
      "    '''\n",
      "    dm = numpy.asarray(dm, order='C')\n",
      "    dm_shape = dm.shape\n",
      "    dm_dtype = dm.dtype\n",
      "    nao = dm_shape[-1]\n",
      "\n",
      "    if dm_dtype == numpy.complex128:\n",
      "        dm = numpy.vstack((dm.real, dm.imag)).reshape(-1,nao,nao)\n",
      "        hermi = 0\n",
      "\n",
      "    with mol.with_range_coulomb(omega):\n",
      "        vj, vk = _vhf.direct(dm, mol._atm, mol._bas, mol._env,\n",
      "                             vhfopt, hermi, mol.cart, with_j, with_k)\n",
      "\n",
      "    if dm_dtype == numpy.complex128:\n",
      "        if with_j:\n",
      "            vj = vj.reshape((2,) + dm_shape)\n",
      "            vj = vj[0] + vj[1] * 1j\n",
      "        if with_k:\n",
      "            vk = vk.reshape((2,) + dm_shape)\n",
      "            vk = vk[0] + vk[1] * 1j\n",
      "    else:\n",
      "        if with_j:\n",
      "            vj = vj.reshape(dm_shape)\n",
      "        if with_k:\n",
      "            vk = vk.reshape(dm_shape)\n",
      "    return vj, vk\n",
      "\n",
      "Module(\n",
      "    body=[\n",
      "        FunctionDef(\n",
      "            name='get_jk',\n",
      "            args=arguments(\n",
      "                posonlyargs=[],\n",
      "                args=[\n",
      "                    arg(arg='mol'),\n",
      "                    arg(arg='dm'),\n",
      "                    arg(arg='hermi'),\n",
      "                    arg(arg='vhfopt'),\n",
      "                    arg(arg='with_j'),\n",
      "                    arg(arg='with_k'),\n",
      "                    arg(arg='omega')],\n",
      "                kwonlyargs=[],\n",
      "                kw_defaults=[],\n",
      "                defaults=[\n",
      "                    Constant(value=1),\n",
      "                    Constant(value=None),\n",
      "                    Constant(value=True),\n",
      "                    Constant(value=True),\n",
      "                    Constant(value=None)]),\n",
      "            body=[\n",
      "                Expr(\n",
      "                    value=Constant(value=\"Compute J, K matrices for all input density matrices\\n\\n    Args:\\n        mol : an instance of :class:`Mole`\\n\\n        dm : ndarray or list of ndarrays\\n            A density matrix or a list of density matrices\\n\\n    Kwargs:\\n        hermi : int\\n            Whether J, K matrix is hermitian\\n\\n            | 0 : not hermitian and not symmetric\\n            | 1 : hermitian or symmetric\\n            | 2 : anti-hermitian\\n\\n        vhfopt :\\n            A class which holds precomputed quantities to optimize the\\n            computation of J, K matrices\\n\\n        with_j : boolean\\n            Whether to compute J matrices\\n\\n        with_k : boolean\\n            Whether to compute K matrices\\n\\n        omega : float\\n            Parameter of range-seperated Coulomb operator: erf( omega * r12 ) / r12.\\n            If specified, integration are evaluated based on the long-range\\n            part of the range-seperated Coulomb operator.\\n\\n    Returns:\\n        Depending on the given dm, the function returns one J and one K matrix,\\n        or a list of J matrices and a list of K matrices, corresponding to the\\n        input density matrices.\\n\\n    Examples:\\n\\n    >>> from pyscf import gto, scf\\n    >>> from pyscf.scf import _vhf\\n    >>> mol = gto.M(atom='H 0 0 0; H 0 0 1.1')\\n    >>> dms = numpy.random.random((3,mol.nao_nr(),mol.nao_nr()))\\n    >>> j, k = scf.hf.get_jk(mol, dms, hermi=0)\\n    >>> print(j.shape)\\n    (3, 2, 2)\\n    \")),\n",
      "                Assign(\n",
      "                    targets=[\n",
      "                        Name(id='dm', ctx=Store())],\n",
      "                    value=Call(\n",
      "                        func=Attribute(\n",
      "                            value=Name(id='numpy', ctx=Load()),\n",
      "                            attr='asarray',\n",
      "                            ctx=Load()),\n",
      "                        args=[\n",
      "                            Name(id='dm', ctx=Load())],\n",
      "                        keywords=[\n",
      "                            keyword(\n",
      "                                arg='order',\n",
      "                                value=Constant(value='C'))])),\n",
      "                Assign(\n",
      "                    targets=[\n",
      "                        Name(id='dm_shape', ctx=Store())],\n",
      "                    value=Attribute(\n",
      "                        value=Name(id='dm', ctx=Load()),\n",
      "                        attr='shape',\n",
      "                        ctx=Load())),\n",
      "                Assign(\n",
      "                    targets=[\n",
      "                        Name(id='dm_dtype', ctx=Store())],\n",
      "                    value=Attribute(\n",
      "                        value=Name(id='dm', ctx=Load()),\n",
      "                        attr='dtype',\n",
      "                        ctx=Load())),\n",
      "                Assign(\n",
      "                    targets=[\n",
      "                        Name(id='nao', ctx=Store())],\n",
      "                    value=Subscript(\n",
      "                        value=Name(id='dm_shape', ctx=Load()),\n",
      "                        slice=UnaryOp(\n",
      "                            op=USub(),\n",
      "                            operand=Constant(value=1)),\n",
      "                        ctx=Load())),\n",
      "                If(\n",
      "                    test=Compare(\n",
      "                        left=Name(id='dm_dtype', ctx=Load()),\n",
      "                        ops=[\n",
      "                            Eq()],\n",
      "                        comparators=[\n",
      "                            Attribute(\n",
      "                                value=Name(id='numpy', ctx=Load()),\n",
      "                                attr='complex128',\n",
      "                                ctx=Load())]),\n",
      "                    body=[\n",
      "                        Assign(\n",
      "                            targets=[\n",
      "                                Name(id='dm', ctx=Store())],\n",
      "                            value=Call(\n",
      "                                func=Attribute(\n",
      "                                    value=Call(\n",
      "                                        func=Attribute(\n",
      "                                            value=Name(id='numpy', ctx=Load()),\n",
      "                                            attr='vstack',\n",
      "                                            ctx=Load()),\n",
      "                                        args=[\n",
      "                                            Tuple(\n",
      "                                                elts=[\n",
      "                                                    Attribute(\n",
      "                                                        value=Name(id='dm', ctx=Load()),\n",
      "                                                        attr='real',\n",
      "                                                        ctx=Load()),\n",
      "                                                    Attribute(\n",
      "                                                        value=Name(id='dm', ctx=Load()),\n",
      "                                                        attr='imag',\n",
      "                                                        ctx=Load())],\n",
      "                                                ctx=Load())],\n",
      "                                        keywords=[]),\n",
      "                                    attr='reshape',\n",
      "                                    ctx=Load()),\n",
      "                                args=[\n",
      "                                    UnaryOp(\n",
      "                                        op=USub(),\n",
      "                                        operand=Constant(value=1)),\n",
      "                                    Name(id='nao', ctx=Load()),\n",
      "                                    Name(id='nao', ctx=Load())],\n",
      "                                keywords=[])),\n",
      "                        Assign(\n",
      "                            targets=[\n",
      "                                Name(id='hermi', ctx=Store())],\n",
      "                            value=Constant(value=0))],\n",
      "                    orelse=[]),\n",
      "                With(\n",
      "                    items=[\n",
      "                        withitem(\n",
      "                            context_expr=Call(\n",
      "                                func=Attribute(\n",
      "                                    value=Name(id='mol', ctx=Load()),\n",
      "                                    attr='with_range_coulomb',\n",
      "                                    ctx=Load()),\n",
      "                                args=[\n",
      "                                    Name(id='omega', ctx=Load())],\n",
      "                                keywords=[]))],\n",
      "                    body=[\n",
      "                        Assign(\n",
      "                            targets=[\n",
      "                                Tuple(\n",
      "                                    elts=[\n",
      "                                        Name(id='vj', ctx=Store()),\n",
      "                                        Name(id='vk', ctx=Store())],\n",
      "                                    ctx=Store())],\n",
      "                            value=Call(\n",
      "                                func=Attribute(\n",
      "                                    value=Name(id='_vhf', ctx=Load()),\n",
      "                                    attr='direct',\n",
      "                                    ctx=Load()),\n",
      "                                args=[\n",
      "                                    Name(id='dm', ctx=Load()),\n",
      "                                    Attribute(\n",
      "                                        value=Name(id='mol', ctx=Load()),\n",
      "                                        attr='_atm',\n",
      "                                        ctx=Load()),\n",
      "                                    Attribute(\n",
      "                                        value=Name(id='mol', ctx=Load()),\n",
      "                                        attr='_bas',\n",
      "                                        ctx=Load()),\n",
      "                                    Attribute(\n",
      "                                        value=Name(id='mol', ctx=Load()),\n",
      "                                        attr='_env',\n",
      "                                        ctx=Load()),\n",
      "                                    Name(id='vhfopt', ctx=Load()),\n",
      "                                    Name(id='hermi', ctx=Load()),\n",
      "                                    Attribute(\n",
      "                                        value=Name(id='mol', ctx=Load()),\n",
      "                                        attr='cart',\n",
      "                                        ctx=Load()),\n",
      "                                    Name(id='with_j', ctx=Load()),\n",
      "                                    Name(id='with_k', ctx=Load())],\n",
      "                                keywords=[]))]),\n",
      "                If(\n",
      "                    test=Compare(\n",
      "                        left=Name(id='dm_dtype', ctx=Load()),\n",
      "                        ops=[\n",
      "                            Eq()],\n",
      "                        comparators=[\n",
      "                            Attribute(\n",
      "                                value=Name(id='numpy', ctx=Load()),\n",
      "                                attr='complex128',\n",
      "                                ctx=Load())]),\n",
      "                    body=[\n",
      "                        If(\n",
      "                            test=Name(id='with_j', ctx=Load()),\n",
      "                            body=[\n",
      "                                Assign(\n",
      "                                    targets=[\n",
      "                                        Name(id='vj', ctx=Store())],\n",
      "                                    value=Call(\n",
      "                                        func=Attribute(\n",
      "                                            value=Name(id='vj', ctx=Load()),\n",
